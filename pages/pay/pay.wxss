/**wxpay.wxss**/
.container{background:#f4f4f4;position:absolute;left:0;right:0;top:0;bottom:0;}
.numKeybord{width:100%;position:absolute;top:50%;bottom:0;right:0;left:0;transition:all .5s;visibility:inherit;}
.num{position:relative;background:#fafafa;color:#444;}
.numKeybord .num:before{
  content:'';display:block;transform:scaleY(0.5);border-top:1px solid #ccc;transform-origin:bottom;
  position:absolute;top:-1px;left:0;right:0;
}
.cursor{height:36px;}
.cursor.blink {
    position:relative;display:inline-block;
}
.cursor.blink:after{ position:absolute;right:-4px;top:50%;width:2px;bottom:0;animation: blink 1s infinite steps(1, start);content: '';
height:36px;margin-top:-18px;
}
.payval{line-height:normal;font-size:36px;}
.numInput{background:#fff;padding:20px 4px;overflow:hidden;}
@keyframes blink {
    0%, 100% {
        background-color: #000;
        color: #aaa;
        opacity:1;
    }
    50% {
        background-color: #fff; /* not #aaa because it's seem there is Google Chrome bug */
        opacity:0;
        color: #000;
    }
}
@keyframes sliderOutDown{
  from{transform: translateY(0);}
  to{transform:translateY(100%); visibility:hidden;}
  
}

.names{cursor:text}
.numKeybord .num:after{
  content:'';display:block;transform:scaleX(0.5);border-left:1px solid #ccc;transform-origin:0 100%;width:0;position:absolute;top:0;bottom:0;left:0;}
.num text{position:absolute;left:0;top:50%;right:0;margin-top:-26rpx;font-size:50rpx;}
.hidekey{transform: translateY(100%);transition-timing-function:ease-out; visibility:hidden;}
.numKey{position:absolute;left:0;bottom:0;top:0;right:25%;z-index:99;}
.num{width:33.333333333%;float:left;height:25%;text-align:center;transition:.3s;}
.numhover{background:#333;color:#fff;}
.keyTool .payBtn{height:75%;background:#1aad19;color:#fff;}
.keyTool .delBtn{height:25%;}
.keyTool .payBtn:after{border:none;}
.keyTool{position:absolute;right:0;bottom:0;left:75%;top:0;}
.keyTool .num{width:100%;text-align:center;}

.mallInfo{display:flex;flex-wrap:wrap;margin-bottom:40rpx;}
.mallImg{width:80rpx;height:80rpx;margin-right:20rpx;border-radius:700rpx;}
.mallField{font-size:24rpx;color:#666;}
.mallInfos text{display:block;}
.payHead{padding:0 40rpx;}
.payNum{border:1px solid #ccc;padding:40rpx 20rpx;border-radius:2rpx;background:#fff;font-size:50rpx;}
.delico{font-size:0;display:block;width:60rpx;height:14rpx;background:#666;}
.payToggle{
    display:flex;justify-content:space-between;align-items:center;margin:20rpx 0;
}
.mallInfo{margin-top:20rpx;}
.topBtn{font-size:30rpx;}
.vipPay{font-size:30rpx;}

.payBtnHover{background:#14A313;}
.toppage{display:none;}
@font-face {
  font-family: 'payico';
  src:
    url(data:application/x-font-ttf;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

[class^="ico-"], [class*=" ico-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'payico' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.ico-delBtn:before {
  content: "\e900";
}
.ico-keyBorder:before {
  content: "\e901";
}


