<!--pages/integral/integral.wxml-->

<view class="integral">
    <view class="integral-head" style="background:{{}}">
            <view class="points">92012</view>
            <view class="icon">?</view>


            <view class="lsimg">
            <image src="../../pages/images/anmin.svg" mode="aspectFit"></image>
            </view>
            <view class="lsimg lsimg2">
            <image src="../../pages/images/anmin.svg" mode="aspectFit"></image>
            </view>
             <view class="lsimg lsimg3">
            <image src="../../pages/images/anmin.svg" mode="aspectFit"></image>
            </view>
    </view>
    <view class="integral-list">
        <view class="integral-item" wx:for="{{integral}}" wx:key="inegral">
            <view class="">
                <view class="type"> {{item.type}}</view>
                <view class="time">{{item.time}}</view>  
            </view>
            <text class="number">{{item.number}}</text>
        </view>
    </view>
</view>