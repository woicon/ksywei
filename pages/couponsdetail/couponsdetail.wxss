/* pages/couponsdetail/couponsdetail.wxss */
page{
    background:#1cc16f;
}
.coupons-detail{
    margin:10% 4%;
    padding:30rpx;
    border-radius:8rpx;
    animation:easyin .5s .2s ease-in both;
}
.coupon-infos{
    margin:40rpx 0;
    border-bottom:1px dashed #eee;
    padding-bottom:30rpx;
}

.coupon-infos view{
    line-height:2;
    color:#ccc;
    font-size:24rpx;
    display:flex;
    align-items:center;
}
.coupon-infos .icon{
    margin-right:6rpx;
}
.cost{
    font-size:30rpx;
    text-align:center;
    color:#FF5C3B; 
    display:flex;
    align-items:center;
    justify-content:center;
}
.cost text{
    font-size:80rpx;
}
.coupon-name{
    font-size:36rpx;margin:30rpx 0;text-align:center;
}
.ewm-img{
    height:400rpx;
}
.ewm-img image{
    width:100%;
    height:100%;
}
.coupons-logo{
    width:60rpx;
    height:60rpx;
    margin:0 auto;
    margin-top:-60rpx;
    border-radius:90rpx;
    overflow:hidden;
}
.coupons-logo image{
    width:100%;
    height:100%;
}
.rqcode{text-align:center;margin:30rpx 0;}
.couponno{line-height:2;color:#666;text-align:center;}
.rqcode-img{
    height:90rpx;width:100%;
}
.rqcode-img image{
    height:100%;width:100%;
}
.nos{
    letter-spacing:14rpx;font-size:26rpx;
}
.coupons-detail{
    background:#fff;
}
.sku{
    display:flex;
    align-items: center;
    justify-content:space-between;

}
.sku-name{
    font-size:32rpx;line-height:2.5;
}
.sku-text{
    font-size:24rpx;color:#999;
}
.title{
    border-top:1px dashed #eee;line-height:3;margin-top:30rpx; font-size:30rpx;
    color:#1cc16f;
}
.description{
    padding:10rpx 0 20rpx 0;
}
.description-text{
    line-height:1.6;color:#666;
}